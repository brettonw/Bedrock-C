{
    "values": {
        "sourcePath": "source",
        "targetPath": "target",
        "objectsPath": "objects",
        "sourceExtension": ".cpp",
        "objectExtension": ".o",
        "dependencyExtension": ".d",
        "target": "*",
        "dependencies": [],
        "type": "application",
        "configuration": "*",
        "verbose": 1
    },
    "configurations": {
        "debug": {
            "compilerOptions": "-std=c++11 -Wall -g -D_DEBUG_",
            "linkerOptions": "",
            "dependerOptions": "-std=c++11 -Wall -g -D_DEBUG_"
        },
        "release": {
            "compilerOptions": "-std=c++11 -Wall -O3 -D_NDEBUG_",
            "linkerOptions": "",
            "dependerOptions": "-std=c++11 -Wall -O3 -D_NDEBUG_"
        }
    },
    "types": {
        "application": {
            "sourceFullPath": "$sourcePath/$target",
            "objectsFullPath": "$targetPath/$target/$configuration/$objectsPath",
            "objectFile": "objectsFullPath/$sourceFile$objectExtension",
            "compiler": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
            "compilerOptions": "$compilerOptions -c $sourceFullPath/$sourceFile$sourceExtension $includes -o $objectFile",
            "dependencyFile": "$objectsFullPath/$sourceFile$dependencyExtension",
            "depender": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
            "dependerOptions": "$dependerOptions -MM $sourceFullPath/$sourceFile$sourceExtension > $dependencyFile",
            "linker": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
            "linkerOptions": "$linkerOptions -o $targetFile $objectsFullPath/*$objectExtension $libraries",
            "targetFile": "$targetPath/$target/$configuration/$target"
        },
        "sharedLibrary": {
            "sourceFullPath": "$sourcePath/$target",
            "objectsFullPath": "$targetPath/$target/$configuration/$objectsPath",
            "objectFile": "$objectsFullPath/$sourceFile$objectExtension",
            "compiler": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
            "compilerOptions": "$compilerOptions -fPIC -c $sourceFullPath/$sourceFile$sourceExtension $includes -o $objectFile",
            "dependencyFile": "$objectsFullPath/$sourceFile$dependencyExtension",
            "depender": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
            "dependerOptions": "$compilerOptions -MM $sourceFullPath/$sourceFile$sourceExtension > $dependencyFile",
            "linker": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
            "linkerOptions": "$linkerOptions -shared -o $targetFile $objectsFullPath/*$objectExtension",
            "targetFile": "$targetPath/$target/$configuration/lib$target.so",
            "linkTo": "-L$targetPath/$target/$configuration -l$target",
            "toInclude": "-I$sourceFullPath"
        },
        "staticLibrary": {
            "sourceFullPath": "$sourcePath/$target",
            "objectsFullPath": "$targetPath/$target/$configuration/$objectsPath",
            "objectFile": "objectsFullPath/$sourceFile$objectExtension",
            "compiler": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
            "compilerOptions": "$compilerOptions -c $sourceFullPath/$sourceFile$sourceExtension $includes -o $objectFile",
            "dependencyFile": "$objectsFullPath/$sourceFile$dependencyExtension",
            "depender": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
            "dependerOptions": "$compilerOptions -MM $sourceFullPath/$sourceFile$sourceExtension > $dependencyFile",
            "linker": "$(find /usr/local/bin /usr/bin | grep -P \"\\/ar$\" | head -n 1)",
            "linkerOptions": "$linkerOptions rcs $targetFile $objectsFullPath/*$objectExtension",
            "targetFile": "$targetPath/$target/$configuration/lib$target.a",
            "linkTo": "-L$targetPath/$target/$configuration -l$target",
            "toInclude": "-I$sourceFullPath"
        }
    }
}
