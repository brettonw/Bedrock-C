{
    "buildConfigurationFileName": "build.json",
    "compiler": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
    "linker": "$compiler",
    "depender": "$compiler",
    "sourcePath": "source",
    "targetPath": "target",
    "sourceExtension": ".cpp",
    "objectExtension": ".o",
    "dependencyExtension": ".d",
    "libraryExtension": ".a",
    "target": "*",
    "configuration": "*",
    "type": "application",
    "dependencies": [],
    "configurations": {
        "debug": {
            "compilerOptions": "-std=c++11 -Wall -g -D_DEBUG_",
            "linkerOptions": "",
            "dependerOptions": "-MM"
        },
        "release": {
            "compilerOptions": "-std=c++11 -Wall -O3 -D_NDEBUG_",
            "linkerOptions": "",
            "dependerOptions": "-MM"
        }
    }
}
