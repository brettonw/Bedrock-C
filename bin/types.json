{
    "application": {
        "objectFile": "$targetPath/$target/$configuration/$objectsPath/$sourceFile$objectExtension",
        "compiler": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
        "compilerOptions": "$compilerOptions -c $sourcePath/$target/$sourceFile $includeTo -o $targetPath/$target/$configuration/$objectsPath/$sourceFile$objectExtension",
        "dependencyFile": "$targetPath/$target/$configuration/$objectsPath/$sourceFile$dependencyExtension",
        "depender": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
        "dependerOptions": "$dependerOptions -MM $sourcePath/$target/$sourceFile > $dependencyFile",
        "linker": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
        "linkerOptions": "$linkerOptions -o $targetFile $targetPath/$target/$configuration/$objectsPath/*$objectExtension $linkTo",
        "targetFile": "$targetPath/$target/$configuration/$target"
    },
    "sharedLibrary": {
        "objectFile": "$targetPath/$target/$configuration/$objectsPath/$sourceFile$objectExtension",
        "compiler": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
        "compilerOptions": "$compilerOptions -fPIC -c $sourcePath/$target/$sourceFile $includeTo -o $targetPath/$target/$configuration/$objectsPath/$sourceFile$objectExtension",
        "dependencyFile": "$targetPath/$target/$configuration/$objectsPath/$sourceFile$dependencyExtension",
        "depender": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
        "dependerOptions": "$compilerOptions -MM $sourcePath/$target/$sourceFile > $dependencyFile",
        "linker": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
        "linkerOptions": "$linkerOptions -shared -o $targetFile $targetPath/$target/$configuration/$objectsPath/*$objectExtension",
        "targetFile": "$targetPath/$target/$configuration/lib$target.so",
        "linkTo": "-L$targetPath/$target/$configuration -l$target",
        "includeTo": "-I$sourcePath/$target"
    },
    "staticLibrary": {
        "objectFile": "$targetPath/$target/$configuration/$objectsPath/$sourceFile$objectExtension",
        "compiler": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
        "compilerOptions": "$compilerOptions -c $sourcePath/$target/$sourceFile $includeTo -o $targetPath/$target/$configuration/$objectsPath/$sourceFile$objectExtension",
        "dependencyFile": "$targetPath/$target/$configuration/$objectsPath/$sourceFile$dependencyExtension",
        "depender": "$(find /usr/local/bin /usr/bin | grep -P \"\\/g\\+\\+\" | head -n 1)",
        "dependerOptions": "$compilerOptions -MM $sourcePath/$target/$sourceFile > $dependencyFile",
        "linker": "$(find /usr/local/bin /usr/bin | grep -P \"\\/ar$\" | head -n 1)",
        "linkerOptions": "$linkerOptions rcs $targetFile $targetPath/$target/$configuration/$objectsPath/*$objectExtension",
        "targetFile": "$targetPath/$target/$configuration/lib$target.a",
        "linkTo": "-L$targetPath/$target/$configuration -l$target",
        "includeTo": "-I$sourcePath/$target"
    }
}
